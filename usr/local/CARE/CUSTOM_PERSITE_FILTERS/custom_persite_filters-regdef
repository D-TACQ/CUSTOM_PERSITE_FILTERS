MOD_ID           0x0000 0xffffffff rw %08x
CTRL             0x0004 0xffffffff rw %08x
CTRL.MOD_EN      0x0004 0x00000001 rw %d
CTRL.DSP_RESET   0x0004 0x00000008 rw %d
CTRL.EN          0x0004 0x00000010 rw %d
CTRL.DSP_BYPASS  0x0004 0x00000020 rw %d
CTRL.AUTO_RESET  0x0004 0x00000080 rw %d
STA              0x0008 0xffffffff rw %08x

